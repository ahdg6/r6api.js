{
  "name": "r6api.js",
  "version": "4.4.1",
  "author": "Daniel Werg",
  "description": "Node.js wrapper around Rainbow Six: Siege API.",
  "license": "MIT",
  "repository": "danielwerg/r6api.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gen-esm-wrapper dist/index.js dist/index.mjs",
    "build:prepare": "path-exists dist || npm run build",
    "tables": "ts-node scripts/tables",
    "methods": "ts-node scripts/methods",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "lint": "eslint src scripts",
    "lint:fix": "eslint src scripts --fix",
    "prepare": "npm run build:prepare"
  },
  "prettier": "@danielwerg/prettier-config",
  "dependencies": {
    "axios": "^1.6.2",
    "r6data": "^0.0.0-alpha.9"
  },
  "devDependencies": {
    "@danielwerg/eslint-config": "^1.0.0",
    "@danielwerg/prettier-config": "^1.0.0",
    "@danielwerg/tsconfig": "^0.1.0",
    "@donedeal0/superdiff": "^1.0.9",
    "@types/node": "^20.10.0",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv": "^16.3.1",
    "eslint": "8.54.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "gen-esm-wrapper": "^1.1.3",
    "path-exists-cli": "^2.0.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "yargs-parser": "^21.1.1"
  },
  "files": [
    "dist",
    "docs"
  ],
  "keywords": [
    "api",
    "wrapper",
    "ubisoft",
    "rainbow",
    "six",
    "siege",
    "rainbowsixsiege",
    "rainbow6",
    "rainbow6siege",
    "r6s"
  ],
  "packageManager": "yarn@4.0.2+sha256.825003a0f561ad09a3b1ac4a3b3ea6207af2796d54f62a9420520915721f5186"
}
